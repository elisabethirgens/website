    "format": "npm run prettier -- --write",
    "lint:prettier": "./scripts/lint-prettier.sh",
    "lint:js": "eslint src",
    "eslint-check":
      "eslint --print-config .eslintrc.yml | eslint-config-prettier-check"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "^0.0.20",
    "bulma": "^0.7.1",
    "classnames": "^2.2.6",
    "font-awesome": "^4.7.0",
    "gatsby": "^1.9.261",
    "gatsby-link": "^1.6.44",
    "gatsby-plugin-netlify": "^1.0.19",
    "gatsby-plugin-netlify-cms": "^2.0.1",
    "gatsby-plugin-postcss-sass": "^1.0.21",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-sass": "^1.0.26",
    "gatsby-plugin-sharp": "^1.6.44",
    "gatsby-remark-images": "^1.5.63",
    "gatsby-source-filesystem": "^1.5.36",
    "gatsby-transformer-remark": "^1.7.41",
    "gatsby-transformer-sharp": "^1.6.24",
    "lodash": "^4.17.10",
    "lodash-webpack-plugin": "^0.11.5",
    "netlify-cms": "1.7.x",
    "postcss-nested": "^3.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-helmet": "^5.2.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "gatsby-plugin-eslint": "^1.0.3",
    "gatsby-plugin-react-next": "^1.0.11",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "prettier": "^1.12.1"
  },
