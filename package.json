{
  "name": "www",
  "description": "OMS' website",
  "version": "1.0.0",
  "author": "Oslo Market Solutions",
  "private": true,
  "scripts": {
    "start": "npm run develop",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged",
    "prettier":
